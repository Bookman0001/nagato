{
  "name": "sirius-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "build": "pathpida --enableStatic --ignorePath .gitignore && next build",
    "lint": "eslint './**/*.ts{,x}'",
    "lint-style": "stylelint ./**/*.{ts,tsx}",
    "prettier": "prettier --check **/**/*.{ts,tsx}",
    "format": "prettier --write **/**/*.{ts,tsx}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "compiler-check": "tsc --noEmit",
    "jest": "jest --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "eslint",
      "prettier"
    ],
    "*": "stylelint ./**/*.{ts,tsx}"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@sendgrid/mail": "7.7.0",
    "axios": "1.1.3",
    "dayjs": "1.11.6",
    "gray-matter": "4.0.3",
    "next": "12.3.2",
    "next-test-api-route-handler": "3.1.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.39.1",
    "react-test-renderer": "18.2.0",
    "remark": "14.0.2",
    "remark-html": "15.0.1",
    "sanitize-html": "2.7.3",
    "styled-components": "5.3.6",
    "swr": "1.3.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@next/eslint-plugin-next": "12.3.2",
    "@storybook/addon-a11y": "6.5.13",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/react": "6.5.13",
    "@testing-library/cypress": "8.0.3",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/axios": "0.14.0",
    "@types/jest": "29.2.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-test-renderer": "18.0.0",
    "@types/sanitize-html": "2.6.2",
    "@types/styled-components": "5.1.26",
    "babel-loader": "9.1.0",
    "babel-plugin-styled-components": "2.0.7",
    "cypress": "10.11.0",
    "eslint": "8.26.0",
    "eslint-config-next": "12.3.2",
    "eslint-config-tenryu": "1.2.6",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.2.2",
    "jest-environment-jsdom": "29.2.2",
    "lint-staged": "13.0.3",
    "mockdate": "3.0.5",
    "msw": "0.47.4",
    "npm-run-all": "4.1.5",
    "pathpida": "0.18.1",
    "postcss-jsx": "0.36.4",
    "postcss-syntax": "0.36.2",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.1",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "26.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
