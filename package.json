{
  "name": "sirius-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "build": "pathpida --enableStatic --ignorePath .gitignore && next build",
    "lint": "eslint './**/*.ts{,x}'",
    "lint:fix": "eslint './**/*.ts{,x}' --fix",
    "prettier": "prettier --check ./**/*.{ts,tsx}",
    "format": "prettier --write ./**/*.{ts,tsx}",
    "compiler-check": "tsc --noEmit",
    "knip": "knip --config knip.json --exclude devDependencies,unlisted",
    "jest": "jest --ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "eslint",
      "prettier"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "5.0.1",
    "@vercel/speed-insights": "1.2.0",
    "axios": "1.9.0",
    "dayjs": "1.11.13",
    "next": "15.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.56.2",
    "sanitize-html": "2.16.0",
    "styled-components": "6.1.17",
    "swr": "2.3.3",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@next/eslint-plugin-next": "15.3.1",
    "@testing-library/cypress": "10.0.3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.12",
    "@types/react": "19.1.3",
    "@types/react-test-renderer": "19.1.0",
    "@types/sanitize-html": "2.16.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.32.0",
    "@typescript-eslint/parser": "8.32.0",
    "babel-loader": "10.0.0",
    "babel-plugin-styled-components": "2.1.4",
    "cypress": "14.3.2",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-cypress": "4.3.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fixed-jsdom": "0.0.9",
    "knip": "5.53.0",
    "lint-staged": "15.5.1",
    "mockdate": "3.0.5",
    "msw": "2.7.6",
    "next-router-mock": "0.9.13",
    "next-test-api-route-handler": "4.0.16",
    "npm-run-all2": "8.0.1",
    "pathpida": "0.25.0",
    "prettier": "3.5.3",
    "react-test-renderer": "19.1.0",
    "simple-git-hooks": "2.13.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "undici": "5.29.0"
  }
}
