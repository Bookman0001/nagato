{
  "name": "sirius-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "build": "pathpida --enableStatic --ignorePath .gitignore && next build",
    "lint": "eslint './**/*.ts{,x}'",
    "lint:fix": "eslint './**/*.ts{,x}' --fix",
    "lint-style": "stylelint ./**/*.{ts,tsx}",
    "prettier": "prettier --check **/**/*.{ts,tsx}",
    "format": "prettier --write **/**/*.{ts,tsx}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "compiler-check": "tsc --noEmit",
    "jest": "jest --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "eslint",
      "prettier"
    ],
    "*": "stylelint ./**/*.{ts,tsx}"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@sendgrid/mail": "7.7.0",
    "axios": "1.2.3",
    "dayjs": "1.11.7",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.42.1",
    "sanitize-html": "2.8.1",
    "styled-components": "5.3.6",
    "swr": "2.0.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@next/eslint-plugin-next": "13.1.2",
    "@storybook/addon-a11y": "6.5.15",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/react": "6.5.15",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/axios": "0.14.0",
    "@types/jest": "29.2.6",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-test-renderer": "18.0.0",
    "@types/sanitize-html": "2.8.0",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-loader": "9.1.2",
    "babel-plugin-styled-components": "2.0.7",
    "cypress": "12.3.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "mockdate": "3.0.5",
    "msw": "0.49.3",
    "next-router-mock": "0.8.0",
    "next-test-api-route-handler": "3.1.8",
    "npm-run-all": "4.1.5",
    "pathpida": "0.20.1",
    "postcss-jsx": "0.36.4",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.3",
    "react-test-renderer": "^18.2.0",
    "simple-git-hooks": "2.8.1",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "26.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
