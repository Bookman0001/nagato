{
  "name": "sirius-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "build": "pathpida --enableStatic --ignorePath .gitignore && next build",
    "lint": "eslint './**/*.ts{,x}'",
    "lint:fix": "eslint './**/*.ts{,x}' --fix",
    "lint-style": "stylelint ./**/*.{ts,tsx}",
    "prettier": "prettier --check ./**/*.{ts,tsx}",
    "format": "prettier --write ./**/*.{ts,tsx}",
    "compiler-check": "tsc --noEmit",
    "knip": "knip --config knip.json --exclude devDependencies,unlisted",
    "jest": "jest --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "eslint",
      "prettier"
    ],
    "*": "stylelint ./**/*.{ts,tsx}"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@sendgrid/mail": "8.1.1",
    "@vercel/speed-insights": "^1.0.2",
    "axios": "1.6.7",
    "dayjs": "1.11.10",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.50.1",
    "sanitize-html": "2.12.1",
    "styled-components": "6.1.8",
    "swr": "2.2.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@next/eslint-plugin-next": "14.0.4",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/cypress": "10.0.1",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/axios": "0.14.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.22",
    "@types/react": "18.2.61",
    "@types/react-test-renderer": "18.0.7",
    "@types/sanitize-html": "2.11.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-loader": "9.1.3",
    "babel-plugin-styled-components": "2.1.4",
    "cypress": "13.6.6",
    "eslint": "8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "knip": "5.0.2",
    "lint-staged": "15.2.2",
    "mockdate": "3.0.5",
    "msw": "2.2.2",
    "next-router-mock": "0.9.12",
    "next-test-api-route-handler": "3.2.0",
    "npm-run-all2": "6.1.2",
    "pathpida": "0.22.0",
    "postcss-syntax": "0.36.2",
    "prettier": "3.2.5",
    "react-test-renderer": "^18.2.0",
    "simple-git-hooks": "2.9.0",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "35.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "undici": "5.28.3"
  }
}
