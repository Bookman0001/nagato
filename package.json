{
  "name": "sirius-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "build": "pathpida --enableStatic --ignorePath .gitignore && next build",
    "lint": "eslint './**/*.ts{,x}'",
    "lint:fix": "eslint './**/*.ts{,x}' --fix",
    "lint-style": "stylelint ./**/*.{ts,tsx}",
    "prettier": "prettier --check ./**/*.{ts,tsx}",
    "format": "prettier --write ./**/*.{ts,tsx}",
    "compiler-check": "tsc --noEmit",
    "knip": "knip --config knip.json --exclude devDependencies,unlisted",
    "jest": "jest --ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "eslint",
      "prettier"
    ],
    "*": "stylelint ./**/*.{ts,tsx}"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@sendgrid/mail": "8.1.3",
    "@vercel/speed-insights": "^1.0.2",
    "axios": "1.7.7",
    "dayjs": "1.11.13",
    "next": "14.2.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "sanitize-html": "2.13.0",
    "styled-components": "6.1.13",
    "swr": "2.2.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@next/eslint-plugin-next": "14.2.12",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/cypress": "10.0.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/axios": "0.14.0",
    "@types/jest": "29.5.13",
    "@types/node": "20.16.5",
    "@types/react": "18.3.7",
    "@types/react-test-renderer": "18.3.0",
    "@types/sanitize-html": "2.13.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-loader": "9.2.1",
    "babel-plugin-styled-components": "2.1.4",
    "cypress": "13.14.2",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.12",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-cypress": "3.5.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "17.10.3",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "knip": "5.30.2",
    "lint-staged": "15.2.10",
    "mockdate": "3.0.5",
    "msw": "2.4.3",
    "next-router-mock": "0.9.13",
    "next-test-api-route-handler": "3.2.0",
    "npm-run-all2": "6.2.3",
    "pathpida": "0.22.0",
    "postcss-syntax": "0.36.2",
    "prettier": "3.3.3",
    "react-test-renderer": "^18.2.0",
    "simple-git-hooks": "2.11.1",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "35.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.6.2",
    "undici": "5.28.4"
  }
}
